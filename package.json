{
  "name": "defi-protocol",
  "version": "2.0.0",
  "description": "A comprehensive DeFi protocol with staking, liquidity pools, yield farming, governance, multi-chain support, and mobile dApp",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:governance": "hardhat run scripts/deploy-governance.js",
    "deploy:multichain": "hardhat run scripts/deploy-multichain.js",
    "node": "hardhat node",
    "coverage": "hardhat coverage",
    "verify": "hardhat verify",
    "mobile:start": "cd mobile-dapp && npm start",
    "mobile:android": "cd mobile-dapp && npm run android",
    "mobile:ios": "cd mobile-dapp && npm run ios"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "@openzeppelin/contracts-upgradeable": "^4.9.3",
    "ethers": "^5.7.2",
    "@chainlink/contracts": "^0.8.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.4"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "hardhat": "^2.17.1",
    "dotenv": "^16.3.1",
    "solidity-coverage": "^0.8.4",
    "chai": "^4.3.7",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1"
  },
  "keywords": [
    "defi",
    "ethereum",
    "smart-contracts",
    "governance",
    "dao",
    "staking",
    "yield-farming",
    "liquidity-pools",
    "multichain",
    "mobile-dapp",
    "analytics"
